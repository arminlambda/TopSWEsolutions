class Solution {
    public boolean isPowerOfThree(int n) {
        

        int[] doTri = new int[20];

        for(int i = 0; i<20; i++) {
            doTri[i] = (int) (Math.pow(3, i));
        }

        for(int j = 0; j<doTri.length; j++) {
            if(n == doTri[j]) {
                return true;
            }
        }return false;
    }
}
